{
  "dependencies": {
    "prettier": "^3.4.2"
  },
  "devDependencies": {},
  "scripts": {
    "dev": "docker-compose up --watch",
    "build": "docker-compose up --build -d",
    "stop-all": "docker-compose down",
    "lint": "prettier . --write",
    "docker-images": "docker ps",
